{
  "name": "amery",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src -D -d dist --compact true",
    "csscomb": "csscomb css/layout.css",
    "watch": "babel src -D -d dist -w",
    "prestart": "npm run build",
    "start": "electron ./",
    "compress": "./script/compress.sh",
    "package": "npm run build && ./script/package.sh",
    "release": "npm run package && npm run compress"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.33.0",
    "gulp": "^3.9.0"
  },
  "dependencies": {
    "electron-rpc": "^1.0.3",
    "lodash": "^3.10.1",
    "menubar": "^2.2.1",
    "path": "^0.12.7",
    "react": "^0.13.3"
  }
}
